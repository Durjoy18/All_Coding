#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a,b;
    cin>>a>>b;
    int r = b-a;
    if(r>0)
    {
        cout<<"You can booked your profit amount:"<<r<<endl;
    }
    else if (r==0)
    {
        cout<<"No Profit, No Loss"<<endl;
    }

    else
    {
        cout<<"Loss:"<<a-b<<endl;
    }
    return 0;
}
