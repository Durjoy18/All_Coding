#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    if ( (a+b)<=c || (b+c)<=a || (c+a)<=b)
    {
        cout<<"Not possible to build a triangle";
    }
   else if(a==b==c)
    {
        cout<<"Equilateral";
    }
    else if (a==b || b==c || c==a )
    {
        cout<<"Isosceles";
    }

    else
    {
        cout<<"Scalene";
    }
    return 0;
}
