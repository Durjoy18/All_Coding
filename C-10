#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,i,j,flag,sum=0;
    cin>>n;
    for(i=2;i<n;i++)
    {
        flag = 1;
    for(j=2;j<=i/2;j++)
    {
        if(i%j==0)
        {
            flag=0;
            break;
        }
    }
    if(flag==1)
    {
        sum+=i;
    }
    }
    cout<<sum;

    return 0;
}
